{
  "name": "jscoverage",
  "version": "0.5.9",
  "description": "a javascript coverage tool, can be used in node dev, and browser side js dev",
  "main": "index.js",
  "bin": {
    "jscoverage": "./bin/jscoverage"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/_mocha -t 60000 -r ./index.js -R spec --covinject true test/"
  },
  "engines": {
    "node" : ">=0.8"
  },
  "dependencies" : {
    "uglify-js" : "2.4.15",
    "optimist" : "0.3.1",
    "xfs" : "0.1.8",
    "ejs": "1.0.0",
    "debug": "1.0.3",
    "coffee-script": ">=1.7.0"
  },
  "devDependencies" : {
    "mocha" : "*",
    "expect.js" : "*",
    "xfs" : "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fishbar/jscoverage.git"
  },
  "keywords": [
    "jscoverage",
    "node",
    "javascript",
    "coverage",
